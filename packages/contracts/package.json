{
  "name": "@director_v2/contracts",
  "version": "0.0.1",
  "description": "oRPC contract definitions for Director v2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "biome check . && oxlint",
    "lint:types": "tsc --noEmit"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/contract": "catalog:",
    "@orpc/zod": "catalog:",
    "zod": "catalog:",
    "@director_v2/db": "workspace:*"
  }
}
