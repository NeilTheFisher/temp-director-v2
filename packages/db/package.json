{
  "name": "@director_v2/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./prisma/*": {
      "types": "./prisma/*.ts",
      "import": "./prisma/*.ts"
    }
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:studio": "prisma studio --browser none",
    "lint": "biome check . && oxlint",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "catalog:",
    "prisma-zod-generator": "^1.31.7",
    "zod": "catalog:",
    "zod-prisma-types": "^3.3.5"
  },
  "devDependencies": {
    "prisma": "^6.19.0",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
