{
  "name": "@director_v2/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./prisma/*": {
      "types": "./prisma/*.ts",
      "import": "./prisma/*.ts"
    }
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --create-only",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:studio": "prisma studio --browser none",
    "lint": "biome check . && oxlint --type-aware",
    "lint:types": "tsc -b"
  },
  "dependencies": {
    "@director_v2/config": "workspace:*",
    "@prisma/adapter-mariadb": "^7.0.0",
    "@prisma/client": "catalog:",
    "@prisma/client-runtime-utils": "^7.0.0",
    "ioredis": "^5.8.2",
    "prisma-zod-generator": "^2.1.2",
    "zod": "catalog:",
    "zod-prisma-types": "^3.3.5"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "prisma": "^7.0.0",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
