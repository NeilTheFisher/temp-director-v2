generator client {
  provider     = "prisma-client"
  output       = "../generated"
  moduleFormat = "esm"
}

generator zod {
  provider         = "zod-prisma-types"
  output           = "../generated/zod"
  prismaClientPath = "../client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
