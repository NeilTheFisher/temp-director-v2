model User {
  id                                  BigInt                    @id @default(autoincrement()) @db.UnsignedBigInt
  name                                String?
  email                               String?                   @unique(map: "user_email_unique")
  email_verified_at                   DateTime?                 @db.Timestamp(0)
  password                            String
  remember_token                      String?                   @db.VarChar(100)
  created_at                          DateTime?                 @db.Timestamp(0)
  updated_at                          DateTime?                 @db.Timestamp(0)
  created_by                          Int?
  msisdn                              String?                   @unique(map: "user_msisdn_unique")
  otp                                 String?
  otp_created_at                      DateTime?                 @db.Timestamp(0)
  personal_group_id                   BigInt?                   @db.UnsignedBigInt
  image_uid                           String?                   @db.Char(36)
  verif_code                          Int?
  verif_expir                         DateTime?                 @db.Timestamp(0)
  type                                String                    @default("user")
  timezone                            String?
  avatar_url                          String?
  account_type                        Int                       @default(1)
  is_deleted                          Boolean                   @default(false)
  deleted_timestamp                   Int?
  session_otp                         Int?
  email_user                          email_user[]
  event_requests                      event_requests[]
  event_user                          event_user[]              @ignore
  group_group_owner_idTouser          group[]                   @relation("group_owner_idTouser")
  group_user                          group_user[]              @ignore
  kpi_emotes_sent                     kpi_emotes_sent[]
  kpi_questions_sent                  kpi_questions_sent[]
  model_logs                          model_logs[]
  roles_user_group                    roles_user_group[]        @ignore
  session_logs                        session_logs[]
  stream_url_token                    stream_url_token[]
  group_user_personal_group_idTogroup group?                    @relation("user_personal_group_idTogroup", fields: [personal_group_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "user_personal_group_id_foreign")
  users_blocked_by_users              users_blocked_by_users[]
  users_reported_by_users             users_reported_by_users[]

  @@index([msisdn], map: "user_msisdn_index")
  @@index([personal_group_id], map: "user_personal_group_id_foreign")
}
